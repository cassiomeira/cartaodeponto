{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "image": "latest",
        "gradleCommand": ":app:bundleRelease",
        "env": {
          "JAVA_HOME": "/usr/lib/jvm/java-17-openjdk-amd64"
        }
      }
    }
  },
  "submit": {
    "production": {}
  }
}